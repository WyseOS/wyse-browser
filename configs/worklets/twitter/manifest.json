{
  "type": "worklet",
  "name": "Twitter",
  "version": "0.1.0",
  "dependencies": [],
  "actions": [
    {
      "name": "action_login",
      "description": "Twitter login",
      "cmd_input": [
        {
          "name": "username",
          "value_type": "string"
        },
        {
          "name": "password",
          "value_type": "string"
        }
      ],
      "cmd_output": [
        {
          "name": "msg",
          "value_type": "string"
        }
      ]
    },
    {
      "name": "action_input_otp",
      "description": "Twitter input otp",
      "cmd_input": [
        {
          "name": "otp",
          "value_type": "string"
        }
      ],
      "cmd_output": [
        {
          "name": "msg",
          "value_type": "string"
        }
      ]
    },
    {
      "name": "action_search",
      "description": "Search tweets from Twitter",
      "cmd_input": [
        {
          "name": "searchKeyword",
          "value_type": "string"
        },
        {
          "name": "targetTweetCount",
          "value_type": "number"
        }
      ],
      "cmd_output": [
        {
          "name": "fileFullPath",
          "value_type": "string"
        }
      ]
    },
    {
      "name": "action_send_tweet",
      "description": "Send a tweet to Twitter",
      "cmd_input": [
        {
          "name": "tweet",
          "value_type": "string"
        }
      ],
      "cmd_output": [
        {
          "name": "status_url",
          "value_type": "string"
        }
      ]
    },
    {
      "name": "action_like",
      "description": "Like a tweet on Twitter",
      "cmd_input": [
        {
          "name": "tweet_url",
          "value_type": "string"
        }
      ],
      "cmd_output": [
        {
          "name": "result",
          "value_type": "string"
        }
      ]
    },
    {
      "name": "action_retweet",
      "description": "Retweet a tweet on Twitter",
      "cmd_input": [
        {
          "name": "tweet_url",
          "value_type": "string"
        }
      ],
      "cmd_output": [
        {
          "name": "result",
          "value_type": "string"
        }
      ]
    },
    {
      "name": "action_get_help_document",
      "description": "fetch help documents",
      "cmd_input": [],
      "cmd_output": [
        {
          "name": "error",
          "value_type": "string"
        }
      ]
    }
  ],
  "properties": {
    "twitter_auth_token": ""
  },
  "base_dir": "worklets/twitter/src"
}
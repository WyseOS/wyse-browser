{
  "name": "wyse-browser",
  "description": "wyse browser and runtime",
  "version": "0.1.1",
  "files": [
    "dist/"
  ],
  "dependencies": {
    "@nestjs/common": "^10.4.13",
    "@nestjs/core": "^10.4.13",
    "@nestjs/platform-express": "^10.4.13",
    "@playwright/test": "^1.54.1",
    "ali-oss": "^6.22.0",
    "axios": "^1.7.9",
    "chromium-bidi": "^0.5.24",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dayjs": "^1.11.13",
    "fingerprint-generator": "^2.1.67",
    "fingerprint-injector": "^2.1.67",
    "lodash": "^4.17.21",
    "papaparse": "^5.4.1",
    "playwright": "^1.54.1",
    "playwright-extra": "^4.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "turndown": "^7.2.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@hey-api/client-fetch": "^0.5.7",
    "@nestjs/cli": "^10.4.9",
    "@nestjs/testing": "^11.1.3",
    "@swc/core": "^1.12.11",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "^22.16.3",
    "@types/papaparse": "^5.3.16",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.5.1",
    "jest": "^30.0.4",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "scripts": {
    "postinstall": "npx playwright install chromium",
    "prepublish": "pnpm build",
    "build": "pnpm build:api && pnpm copy-assets",
    "build:api": "nest build",
    "copy-assets": "mkdir -p dist/engine/src/constants && cp src/constants/page_script.js dist/engine/src/constants/",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/engine/src/index",
    "start:dev": "pnpm copy-assets && nest start --watch --entryFile engine/src/index",
    "start:debug": "nest start --debug --watch --entryFile engine/src/index",
    "start:prod": "node dist/engine/src/index",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  }
}
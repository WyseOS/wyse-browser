{
    "name": "coingecko_trusted_tokens_by_network",
    "type": "flow",
    "description": "Get all trusted tokens by network. Supported networks are: all, ethereum, optimism, polygon, base, arbitrum, gnosis and zksync.",
    "version": "0.1.0",
    "webpage_url": "coingecko.com",
    "nodes": [
        {
            "name": "FlowStart",
            "type": "worklet",
            "version": "0.1.0"
        },
        {
            "name": "Coingecko",
            "type": "worklet",
            "version": "0.1.0",
            "properties": {
                "coingecko_access_key": "$CoingeckoAccessKey"
            }
        },
        {
            "name": "FlowEnd",
            "type": "worklet",
            "version": "0.1.0"
        }
    ],
    "connections": [
        {
            "name": "start the flow, get all trusted tokens by network",
            "src": {
                "worklet": "FlowStart",
                "action": "action_flow_start"
            },
            "dest": [
                {
                    "worklet": "Coingecko",
                    "action": "action_trusted_tokens_by_network"
                }
            ]
        },
        {
            "name": "flow ends",
            "src": {
                "worklet": "Coingecko",
                "action": "action_trusted_tokens_by_network"
            },
            "dest": [
                {
                    "worklet": "FlowEnd",
                    "action": "action_flow_end"
                }
            ]
        }
    ],
    "properties": {
        "coingecko_access_key": "$CoingeckoAccessKey"
    },
    "parameters": [
        {
            "name": "network",
            "value_type": "string"
        }
    ]
}
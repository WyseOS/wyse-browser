{
    "name": "twitter_like_retweet",
    "type": "flow",
    "description": "Like and retweet a tweet on Twitter with twitter_auth_token.",
    "version": "0.1.0",
    "webpage_url": "x.com",
    "nodes": [
        {
            "name": "FlowStart",
            "type": "worklet",
            "version": "0.1.0",
            "action": []
        },
        {
            "name": "Twitter",
            "type": "worklet",
            "version": "0.1.0",
            "properties": {
                "twitter_auth_token": "$TwitterAuthToken"
            }
        },
        {
            "name": "FlowEnd",
            "type": "worklet",
            "version": "0.1.0"
        }
    ],
    "connections": [
        {
            "name": "start the flow, like a tweet",
            "src": {
                "worklet": "FlowStart",
                "action": "action_flow_start"
            },
            "dest": [
                {
                    "worklet": "Twitter",
                    "action": "action_like"
                }
            ]
        },
        {
            "name": "retweet a tweet",
            "src": {
                "worklet": "Twitter",
                "action": "action_like"
            },
            "dest": [
                {
                    "worklet": "Twitter",
                    "action": "action_retweet"
                }
            ]
        },
        {
            "name": "flow ends",
            "src": {
                "worklet": "Twitter",
                "action": "action_retweet"
            },
            "dest": [
                {
                    "worklet": "FlowEnd",
                    "action": "action_flow_end"
                }
            ]
        }
    ],
    "properties": {
        "TwitterAuthToken": "$TwitterAuthToken"
    },
    "parameters": [
        {
            "name": "tweet_url",
            "value_type": "string"
        }
    ]
}
{
    "name": "coingecko_search_token",
    "type": "flow",
    "description": "Search for a token by name",
    "version": "0.1.0",
    "webpage_url": "coingecko.com",
    "nodes": [
        {
            "name": "FlowStart",
            "type": "worklet",
            "version": "0.1.0"
        },
        {
            "name": "Coingecko",
            "type": "worklet",
            "version": "0.1.0",
            "properties": {
                "coingecko_access_key": "$CoingeckoAccessKey"
            }
        },
        {
            "name": "FlowEnd",
            "type": "worklet",
            "version": "0.1.0"
        }
    ],
    "connections": [
        {
            "name": "start the flow, search for a token by name",
            "src": {
                "worklet": "FlowStart",
                "action": "action_flow_start"
            },
            "dest": [
                {
                    "worklet": "Coingecko",
                    "action": "action_search_token_by_name"
                }
            ]
        },
        {
            "name": "flow ends",
            "src": {
                "worklet": "Coingecko",
                "action": "action_search_token_by_name"
            },
            "dest": [
                {
                    "worklet": "FlowEnd",
                    "action": "action_flow_end"
                }
            ]
        }
    ],
    "properties": {
        "coingecko_access_key": "$CoingeckoAccessKey"
    },
    "parameters": [
        {
            "name": "name",
            "value_type": "string"
        }
    ]
}
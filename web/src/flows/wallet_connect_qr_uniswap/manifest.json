{
    "name": "wallet_connect_qr_uniswap",
    "type": "flow",
    "description": "Connect to a wallet and scan the qr code. It is specific for WalletConnect on Uniswap.",
    "version": "0.1.0",
    "webpage_url": "uniswap.org",
    "nodes": [
        {
            "name": "FlowStart",
            "type": "worklet",
            "version": "0.1.0"
        },
        {
            "name": "WalletConnect",
            "type": "worklet",
            "version": "0.1.0",
            "properties": {}
        },
        {
            "name": "FlowEnd",
            "type": "worklet",
            "version": "0.1.0"
        }
    ],
    "connections": [
        {
            "name": "start the flow, run wallet connect",
            "src": {
                "worklet": "FlowStart",
                "action": "action_flow_start"
            },
            "dest": [
                {
                    "worklet": "WalletConnect",
                    "action": "action_wallet_connect_get_qrcode_from_uniswap"
                }
            ]
        },
        {
            "name": "return qr code as string",
            "src": {
                "worklet": "WalletConnect",
                "action": "action_wallet_connect_get_qrcode_from_uniswap"
            },
            "dest": [
                {
                    "worklet": "FlowEnd",
                    "action": "action_flow_end"
                }
            ]
        }
    ],
    "properties": {},
    "parameters": []
}
{
    "name": "coingecko_trending_tokens",
    "type": "flow",
    "description": "Query trending search coins, nfts and categories on CoinGecko in the last 24 hours. It supports: Top 15 trending coins (sorted by the most popular user searches), Top 7 trending NFTs (sorted by the highest percentage change in floor prices) and Top 5 trending categories (sorted by the most popular user searches)",
    "version": "0.1.0",
    "webpage_url": "coingecko.com",
    "nodes": [
        {
            "name": "FlowStart",
            "type": "worklet",
            "version": "0.1.0"
        },
        {
            "name": "Coingecko",
            "type": "worklet",
            "version": "0.1.0",
            "properties": {
                "coingecko_access_key": "$CoingeckoAccessKey"
            }
        },
        {
            "name": "FlowEnd",
            "type": "worklet",
            "version": "0.1.0"
        }
    ],
    "connections": [
        {
            "name": "start the flow, query trending search coins, nfts and categories",
            "src": {
                "worklet": "FlowStart",
                "action": "action_flow_start"
            },
            "dest": [
                {
                    "worklet": "Coingecko",
                    "action": "action_trending"
                }
            ]
        },
        {
            "name": "flow ends",
            "src": {
                "worklet": "Coingecko",
                "action": "action_trending"
            },
            "dest": [
                {
                    "worklet": "FlowEnd",
                    "action": "action_flow_end"
                }
            ]
        }
    ],
    "properties": {
        "coingecko_access_key": "$CoingeckoAccessKey"
    },
    "parameters": []
}
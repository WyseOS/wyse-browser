{
    "name": "coingecko_all_categories",
    "type": "flow",
    "description": "Query all the coins categories with market data (market cap, volume, etc.) on CoinGecko which are sorted by the market cap.",
    "version": "0.1.0",
    "webpage_url": "coingecko.com",
    "nodes": [
        {
            "name": "FlowStart",
            "type": "worklet",
            "version": "0.1.0"
        },
        {
            "name": "Coingecko",
            "type": "worklet",
            "version": "0.1.0",
            "properties": {
                "coingecko_access_key": "$CoingeckoAccessKey"
            }
        },
        {
            "name": "FlowEnd",
            "type": "worklet",
            "version": "0.1.0"
        }
    ],
    "connections": [
        {
            "name": "start the flow, query all categories",
            "src": {
                "worklet": "FlowStart",
                "action": "action_flow_start"
            },
            "dest": [
                {
                    "worklet": "Coingecko",
                    "action": "action_list_categories"
                }
            ]
        },
        {
            "name": "flow ends",
            "src": {
                "worklet": "Coingecko",
                "action": "action_list_categories"
            },
            "dest": [
                {
                    "worklet": "FlowEnd",
                    "action": "action_flow_end"
                }
            ]
        }
    ],
    "properties": {
        "coingecko_access_key": "$CoingeckoAccessKey"
    },
    "parameters": []
}